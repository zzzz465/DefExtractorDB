{
    "targetNodes" :
    {
        "pawnsPlural" :
        {
            "Xpath" : "//pawnsPlural",
            "NodeStringReplaceXpaths" : 
            {
                "[\\w]+Def" : "./defName"
            }
        },
        "li_label" :
        {
            "Xpath" : "//li[label]",
            "AdditionalNodes" :
            [
                "./label"
            ],
            "NodeStringReplaceXpaths" :
            {
                "[\\w]+Def" : "./defName"
            }
        },
        "li_description" :
        {
            "Xpath" : "//li[description]",
            "AdditionalNodes" :
            [
                "./description"
            ],
            "NodeStringReplaceXpaths" :
            {
                "[\\w]+Def" : "./defName"
            }
        },
        "Label" :
        {
            "Xpath" : "//label",
            "NodeStringReplaceXpaths" :
            {
                "[a-zA-Z0-9]+Def" : "./defName",
                "li" : "$_Count"
            }
        },
        "Description" :
        {
            "Xpath" : "//description",
            "NodeStringReplaceXpaths" :
            {
                "[a-zA-Z0-9]+Def" : "./defName",
                "li" : "$_Count"
            }
        }
    }
}